<?xml version="1.0" encoding="UTF-8"?>  
<SPICE_INPUT>
   <!--会加一的特殊类-->  
    <SpecialClass>
        <Class>Vsource_AC</Class>
        <Class>Vsource_DC</Class>
        <Class>Inductor</Class>
        <Class>PWLVoltageSource</Class>
    </SpecialClass>
    <!--类的参数-->  
    <INSTANCE>  
        <NAME>Vs</NAME>  
        <PSET>Vsource_AC</PSET>  
        <ELECTRODES>
            <electrode>1</electrode>
            <electrode>0</electrode>
        </ELECTRODES>
        <PARAMETERS>
            <PARAMETER>
                <name>Peak amplitude</name>
                <value>10</value>
            </PARAMETER>
            <PARAMETER>
                <name>Frequency</name>
                <value>50</value>
            </PARAMETER>
            <PARAMETER>
                <name>Phase shift</name>
                <value>0</value>
            </PARAMETER>
        </PARAMETERS> 
    </INSTANCE>
    <INSTANCE>  
        <NAME>D1</NAME>  
        <PSET>Diode</PSET>  
        <ELECTRODES>
            <electrode>1</electrode>
            <electrode>2</electrode>
        </ELECTRODES>
        <PARAMETERS>
            <PARAMETER>
                <name>Ron</name>
                <value>0.001</value>
            </PARAMETER>
            <PARAMETER>
                <name>Vf</name>
                <value>0.8</value>
            </PARAMETER>
        </PARAMETERS> 
    </INSTANCE>
    <INSTANCE>  
        <NAME>L1</NAME>  
        <PSET>Inductor</PSET>  
        <ELECTRODES>
            <electrode>2</electrode>
            <electrode>3</electrode>
        </ELECTRODES>
        <PARAMETERS>
            <PARAMETER>
                <name>Inductance</name>
                <value>1.0e-3</value>
            </PARAMETER>
        </PARAMETERS> 
    </INSTANCE>
    <INSTANCE>  
        <NAME>R1</NAME>  
        <PSET>Resistor</PSET>  
        <ELECTRODES>
            <electrode>3</electrode>
            <electrode>0</electrode>
        </ELECTRODES>
        <PARAMETERS>
            <PARAMETER>
                <name>resistance</name>
                <value>10</value>
            </PARAMETER>
        </PARAMETERS> 
    </INSTANCE>
    <INSTANCE>  
        <NAME>C1</NAME>  
        <PSET>Capacitor</PSET>  
        <ELECTRODES>
            <electrode>3</electrode>
            <electrode>0</electrode>
        </ELECTRODES>
        <PARAMETERS>
            <PARAMETER>
                <name>capacitance</name>
                <value>1.0e-4</value>
            </PARAMETER>
        </PARAMETERS> 
    </INSTANCE>
</SPICE_INPUT>

